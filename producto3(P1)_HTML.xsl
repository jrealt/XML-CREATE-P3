	</xsl:template>
	
	<xsl:template match="contenido">
		<xsl:apply-templates />
	</xsl:template>
	
	<xsl:template match="tema">
		<H1><a name="{@ancla}" /><xsl:value-of select="@titulo" /></H1>
		<xsl:apply-templates />
	</xsl:template>
	
	<xsl:template match="subtema">
		<H2><a name="{@ancla}" /><xsl:value-of select="@titulo" /></H2>
		<xsl:apply-templates />
	</xsl:template>
	
	<xsl:template match="subsubtema">
		<H3><a name="{@ancla}" /><xsl:value-of select="@titulo" /></H3>
		<xsl:apply-templates />
	</xsl:template>
	
	<xsl:template match="negrita">
		<strong><xsl:apply-templates /></strong>
	</xsl:template>
	
	<xsl:template match="cursiva">
		<i><xsl:apply-templates /></i>
	</xsl:template>
	
	<xsl:template match="frase">
		<xsl:apply-templates />
	</xsl:template>
	
	<xsl:template match="parrafo">
		<p><xsl:apply-templates /></p>
	</xsl:template>
	
	<xsl:template match="web">
		<xsl:variable name="enlace"><xsl:value-of select="./@enlace" /></xsl:variable>
		<a href="{$enlace}"><xsl:apply-templates /></a>
	</xsl:template>
	
	<xsl:template match="elemento">
		<li>
			<xsl:apply-templates />
		</li>
	</xsl:template>
	
	<xsl:template match="lista_ordenada">
		<ol class="ordered">
			<xsl:apply-templates />
		</ol>
	</xsl:template>
	
	<xsl:template match="lista_desordenada">
		<ul class="unordered">
			<xsl:apply-templates />
		</ul>
	</xsl:template>
	
	<xsl:template match="imagen">
		<br /><br />
		<xsl:variable name="enlace"><xsl:value-of select="./@enlace" /></xsl:variable>
		<xsl:variable name="descripcion"><xsl:value-of select="." /></xsl:variable>
		<img src="{$enlace}" alt="{$descripcion}"/>
		<br /><br />
	</xsl:template>
	
	<xsl:template match="cabecera">
		<th><xsl:apply-templates /></th>
	</xsl:template>
	
	<xsl:template match="columna">
		<td><xsl:apply-templates /></td>
	</xsl:template>
	
	<xsl:template match="fila">
		<tr>
			<xsl:apply-templates />
		</tr>
	</xsl:template>
	
	<xsl:template match="tabla">
		<br /><br />
		<table>
			<xsl:apply-templates />
		</table>
		<br /><br />
	</xsl:template>
	
</xsl:stylesheet>