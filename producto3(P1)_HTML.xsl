				<div class="wrapAll clearfix">
					<div class="sidebar">
						<div class="navigation">
							<div>
								<img class="imglogo" src='./imagenes/logo.png' alt="logo" />
							</div>
							<ul>
							<xsl:for-each select="contenido/tema">
								<li><a href="#{@ancla}"><strong><H3><xsl:value-of select="./@titulo" /></H3></strong></a></li>
								<xsl:if test = "subtema">
									<ul>
									<xsl:for-each select="subtema">
										<li><a href="#{@ancla}"><H4><xsl:value-of select="./@titulo" /></H4></a></li>
										<xsl:if test = "subsubtema">
											<ul>
											<xsl:for-each select="subsubtema">
												<li><a href="#{@ancla}"><H5><xsl:value-of select="./@titulo" /></H5></a></li>
											</xsl:for-each>
											</ul>
										</xsl:if>
									</xsl:for-each>
									</ul>
								</xsl:if>
						    </xsl:for-each>
							</ul>
						</div>
		
					
					</div>
					<div class="mainsection">
						<div class="article">
							<xsl:apply-templates select="contenido" />
						</div>
						<div class="pagefooter">
							Template by <a href="http://html5-templates.com/" target="_blank" rel="nofollow">HTML5 Templates</a> <!-- Please leave this link unchanged -->
						</div>
					
					
					</div>		
